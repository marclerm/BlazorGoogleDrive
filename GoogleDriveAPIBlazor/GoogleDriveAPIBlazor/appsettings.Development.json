{
  "DetailedErrors": true, // turns on CircuitOptions.DetailedErrors
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug" // turns on SignalR debugging
    }
  },
  "Authentication": {
    "Google": {
      "ClientId": "YOUR_CLIENT_ID_DEV.apps.googleusercontent.com",
      "ClientSecret": "YOUR_CLIENT_SECRET_DEV",
      "RedirectUri": "https://localhost:5001/auth/google/callback",
      "ApiKey": "YOUR_API_KEY_DEV"
    }
  }

}
